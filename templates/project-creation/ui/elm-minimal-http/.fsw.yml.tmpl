desc: Auto generated by fswatch [{{ .Name }}-ui]
triggers:
- name: ""
  pattens:
  - 'elm/src/*.elm'
  - 'elm/src/**/*.elm'
  - '!elm/src/Api/Pb/*'
  env:
    DEBUG: "1"
  cmd: touch ../main.go # this trigger the second fswatch event (see ../hack/run-dev.sh)
  shell: true
  delay: 100ms
  stop_timeout: 500ms
  signal: KILL
  kill_signal: ""
watch_paths:
- .
watch_depth: 5
